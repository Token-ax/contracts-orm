      },
      "initRules": {
        "passIn": ["text", "location"],
        "auto": {
          "sender": "msg.sender",
          "timestamp": "block.timestamp"
        }
      },
      "writeRules": {},
      "readRules": {
        "gets": ["text", "timestamp", "sender", "location"]
      }
    }
  }
